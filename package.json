{
	"name": "agnostic-draggable-bug-fixed",
	"version": "1.0.12",
	"description": "An agnostic set of libraries implementing draggable, droppable and sortable behaviours inspired by the jQuery UI implementation",
	"keywords": [
		"draggable",
		"droppable",
		"sortable",
		"drag",
		"drop",
		"sort",
		"interact",
		"mouse",
		"touch",
		"agnostic",
		"javascript"
	],
	"license": "MIT",
	"homepage": "https://github.com/rozek/agnostic-draggable#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rozek/agnostic-draggable.git"
	},
	"bugs": {
		"url": "https://github.com/rozek/agnostic-draggable/issues"
	},
	"author": {
		"name": "Marcos Pont",
		"email": "marcos.pont@gmail.com"
	},
	"contributors": [
		"Andreas Rozek <a.rozek@gmx.de>"
	],
	"main": "dist/agnostic-draggable.js",
	"scripts": {
		"build": "mkdir -p ./dist && cp ./agnostic-draggable.d.ts ./dist && rollup -c",
		"develop": "mkdir -p ./dist && cp ./agnostic-draggable.d.ts ./dist && rollup -c -w",
		"release": "standard-version",
		"code:format": "prettier --write \"src/**/*.js\"",
		"code:lint": "eslint \"src/**/*.js\"",
		"code:fix": "eslint \"src/**/*.js\" --fix",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@babel/core": "^7.14.3",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.4",
		"@babel/preset-env": "^7.14.4",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-array-includes": "^2.0.3",
		"core-js": "^3.14.0",
		"eslint": "^7.28.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-promise": "^5.1.0",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"prettier": "^2.3.1",
		"rollup": "^2.51.0",
		"rollup-plugin-terser": "^7.0.2",
		"standard-version": "^9.3.0"
	},
	"dependencies": {
		"animejs": "^3.2.1",
		"dom-helpers": "^5.2.1",
		"events": "^3.3.0",
		"lodash": "^4.17.21"
	},
	"engines": {
		"node": ">=10.16.0",
		"npm": ">=6.9.0"
	},
	"lint-staged": {
		"*.js": [
			"eslint --cache --fix",
			"prettier --write"
		]
	}
}
