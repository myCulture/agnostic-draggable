{
	"name": "agnostic-draggable-bug-fixed",
	"version": "1.0.6",
	"description": "An agnostic set of libraries implementing draggable, droppable and sortable behaviours inspired by the jQuery UI implementation",
	"keywords": [
		"draggable",
		"droppable",
		"sortable",
		"drag",
		"drop",
		"sort",
		"interact",
		"mouse",
		"touch",
		"agnostic",
		"javascript"
	],
	"license": "MIT",
	"homepage": "https://github.com/rozek/agnostic-draggable#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rozek/agnostic-draggable.git"
	},
	"bugs": {
		"url": "https://github.com/rozek/agnostic-draggable/issues"
	},
	"author": {
		"name": "Marcos Pont",
		"email": "marcos.pont@gmail.com"
	},
	"contributors":[
		"Andreas Rozek <a.rozek@gmx.de>"
	],
	"main": "dist/agnostic-draggable.js",
	"scripts": {
		"build": "rollup -c",
		"develop": "rollup -c -w",
		"release": "standard-version",
		"code:format": "prettier --write \"src/**/*.js\"",
		"code:lint": "eslint \"src/**/*.js\"",
		"code:fix": "eslint \"src/**/*.js\" --fix"
	},
	"devDependencies": {
		"@babel/core": "^7.13.15",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.13.8",
		"@babel/preset-env": "^7.13.15",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^18.0.0",
		"@rollup/plugin-node-resolve": "^11.2.1",
		"babel-eslint": "^10.1.0",
		"babel-plugin-array-includes": "^2.0.3",
		"core-js": "^3.10.1",
		"eslint": "^7.24.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-promise": "^5.1.0",
		"prettier": "^2.2.1",
		"rollup": "^2.45.2",
		"rollup-plugin-terser": "^7.0.2",
		"standard-version": "^9.2.0"
	},
	"dependencies": {
		"animejs": "^3.2.1",
